<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE mapper
  PUBLIC "-//mybatis.org//DTD Mapper 3.0//EN"
  "http://mybatis.org/dtd/mybatis-3-mapper.dtd">
<mapper namespace="shop.fims.goods.RevenueMapper">
<!-- 모든 축제별 보유물품 대여관리 리스트 - 세입 -->
<select id="revenuelist" resultType="shop.fims.vo.Revenue">
SELECT
*
FROM
Goods_rental_revenue


</select>

<select id="coderevenue" resultType="shop.fims.vo.Revenue">
SELECT
*
FROM
Goods_rental_revenue
WHERE 
goodsren_rev_cd = #{goodsren_rev_cd}


</select>

</mapper>